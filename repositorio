    <name>dateFromApiVuelo</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>orac_api_fuente</connection>
    <sql>SELECT
distinct to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;)) AS id_Date
,to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYY&apos;)) AS id_year
,(select round(to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;Q&apos;))&#47;2) from dual) AS id_semestre
,to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;Q&apos;)) AS id_trimester
,rtrim(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;MONTH&apos;)) AS id_month
,rtrim(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;DAY&apos;)) AS id_weekDay_name
,to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;DD&apos;)) AS id_day
FROM API_VUELO
where to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;)) is not null
order by to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;))
</sql>
    <limit>0</limit>
