  <step>
    <name>apiVuelo</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>orac_api_fuente</connection>
    <sql>Select 
distinct Cantidad.ID_VUELO
, ID_AEROLINEA
, AEROPUERTO_DESTINO
, AEROPUERTO_ORIGEN
, NO_VUELO
, id_Date
, Cantidad.CONTACTO_NOMBRE
, cant_mensaje
, cant_mensaje_aceptado
, cant_mensaje_rechazado
, cant_mensaje_error
, D.CANT_PASAJEROS_DECLARDOS
, cant_pasajeros_recibido
, C.CANT_TRIPULANTES_DECLARADOS
, cant_tripulante_recibido

from
(SELECT
  DISTINCT API_VUELO.ID_VUELO
, ID_AEROLINEA
, AEROPUERTO_DESTINO
, AEROPUERTO_ORIGEN
, NO_VUELO
, to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;)) AS id_Date
, API_MENSAJE.CONTACTO_NOMBRE
, count(*) as cant_mensaje
FROM API_VUELO, API_MENSAJE
where API_VUELO.ID_VUELO=API_MENSAJE.ID_VUELO and API_VUELO.NO_VUELO &lt;&gt; &apos;CUB0000&apos; and API_VUELO.NO_VUELO not like &apos;%_PA&apos;
group by API_VUELO.ID_VUELO, ID_AEROLINEA, AEROPUERTO_DESTINO, AEROPUERTO_ORIGEN, NO_VUELO
, to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;)), API_MENSAJE.CONTACTO_NOMBRE
order by to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;)) asc
, NO_VUELO asc, API_VUELO.ID_VUELO, API_MENSAJE.CONTACTO_NOMBRE desc, count(*) desc
) Cantidad left outer join (select count(distinct id_persona) as cant_pasajeros_recibido, id_vuelo from API_VIAJE_PASAJERO where tipo=&apos;P&apos; group by ID_VUELO) pass on pass.id_vuelo=cantidad.id_vuelo
left outer join (select count(distinct id_persona) as cant_tripulante_recibido, id_vuelo from API_VIAJE_PASAJERO where tipo=&apos;T&apos; group by ID_VUELO) trip on trip.id_vuelo=cantidad.id_vuelo
left outer join (select count(*) as cant_mensaje_aceptado, id_vuelo from API_MENSAJE where estado_mensaje=&apos;Aceptado&apos; group by ID_VUELO) acept on acept.id_vuelo=Cantidad.id_vuelo
left outer join (select count(*) as cant_mensaje_rechazado, id_vuelo from API_MENSAJE where estado_mensaje=&apos;Rechazado&apos; group by ID_VUELO) rech on rech.id_vuelo=Cantidad.id_vuelo
left outer join (select count(*) as cant_mensaje_error, id_vuelo from API_MENSAJE, API_ERROR where API_ERROR.id_mensaje= API_MENSAJE.id_mensaje group by ID_VUELO) err on err.id_vuelo=Cantidad.id_vuelo
, API_MENSAJE C, API_MENSAJE D 

where Cantidad.ID_VUELO=C.ID_VUELO and Cantidad.ID_VUELO=D.ID_VUELO 
and  C.CANT_TRIPULANTES_DECLARADOS = (select max( A.CANT_TRIPULANTES_DECLARADOS) FROM API_MENSAJE A where A.ID_VUELO = C.ID_VUELO)
and  D.CANT_PASAJEROS_DECLARDOS = (select max( A.CANT_PASAJEROS_DECLARDOS) FROM API_MENSAJE A where A.ID_VUELO = D.ID_VUELO)

order by id_Date asc, NO_VUELO asc, Cantidad.ID_VUELO, Cantidad.CONTACTO_NOMBRE desc, cant_mensaje desc
</sql>
    <limit>0</limit>
