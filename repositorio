  <step>
    <name>time from apiVuelo</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>orac_api_fuente</connection>
    <sql>SELECT
distinct to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;)) AS id_Date
FROM API_VUELO
where to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;)) is not null
order by to_number(to_char (TO_DATE(fecha_llegada,&apos;dd-mm-yy&apos;), &apos;YYYYMMDD&apos;))
</sql>
    <limit>0</limit>
