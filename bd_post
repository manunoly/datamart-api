create table "aerolineas"( "id_aerolineas" varchar(38) not null, "codigo_alfa2" varchar(2), "codigo_alfa3" varchar(3), "descripccion" varchar(100), constraint "pk10" primary key ("id_aerolineas") ) ; create table "contacto"( "nombre_contacto" varchar(255) not null, "correo_contacto" varchar(250), "fax_contacto" varchar(25), "telefono_contacto" varchar(25), "agencia_contacto" varchar(100), constraint "pk9" primary key ("nombre_contacto") ) ; create table "hecho_vuelo"( "id_vuelo" varchar(255) not null, "id_aerolineas" varchar(38) not null, "nombre_contacto" varchar(255) not null, "id_aeropuerto_destino" varchar(38) not null, "fecha" int8 not null, "id_aeropuerto_origen" varchar(38) not null, "numero_vuelo" varchar(12) not null, "cant_pasageros_declarados" int4, "cant_pasageros_recibidos" int4, "cant_mensages_recibidos" int2, "cant_mensages_aceptados" int2, "cant_mensages_rechazados" int2, "cant_error_mensages" int2, "cant_tripulantes_declarados" int2, "cant_tripulantes_recibidos" int2, constraint "pk1" primary key ("id_vuelo") ) ; create table "lugar_destino"( "id_aeropuerto_destino" varchar(38) not null, "pais_destino_codigo2" varchar(2), "pais_destino_codigo3" varchar(3), "pais_destino_descripcion" varchar(180), "aeropuerto_destino_codigo2" varchar(2), "aeropuerto_destino_codigo3" varchar(3), "aeropuerto_destino_descripcion" varchar(150), constraint "tc_aeropuerto_pk" primary key ("id_aeropuerto_destino") ) ; create table "lugar_origen"( "id_aeropuerto_origen" varchar(38) not null, "pais_origen_codigo2" varchar(2), "pais_origen_codigo3" varchar(3), "pais_origen_descripcion" varchar(180), "aeropuerto_origen_codigo2" varchar(2), "aeropuerto_origen_codigo3" varchar(3), "aeropuerto_origen_descripcion" varchar(150), constraint "tc_aeropuerto_pk_1" primary key ("id_aeropuerto_origen") ) ; create table "tiempo"( "fecha" int8 not null, "anno" varchar(6), "semestre" varchar(1), "trimeste" varchar(1), "mes" varchar(2), "dia_semana" varchar(10), "dia" varchar(2), constraint "pk2" primary key ("fecha") ) ; alter table "hecho_vuelo" add constraint "reflugar_destino6" foreign key ("id_aeropuerto_destino") references "lugar_destino"("id_aeropuerto_destino") ; alter table "hecho_vuelo" add constraint "reflugar_origen7" foreign key ("id_aeropuerto_origen") references "lugar_origen"("id_aeropuerto_origen") ; alter table "hecho_vuelo" add constraint "refdim_airline7" foreign key ("id_aerolineas") references "aerolineas"("id_aerolineas") ; alter table "hecho_vuelo" add constraint "refdim_contact11" foreign key ("nombre_contacto") references "contacto"("nombre_contacto") ; alter table "hecho_vuelo" add constraint "refdim_time8" foreign key ("fecha") references "tiempo"("fecha") ; a
